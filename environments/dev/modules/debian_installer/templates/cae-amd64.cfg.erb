label onedrive
        menu label ^CAE Install (1 drive)
        kernel d-i/amd64/linux
        append initrd=d-i/amd64/initrd.gz auto url=<%= scope.function_generate("/etc/puppet/auth/getpwd", "urls", "d-i") %>squeeze/./onedrive.cfg console-keymaps-at/keymap=us locale=en_US interface=eth0 netcfg/dhcp_timeout=60 auto=true priority=critical --

